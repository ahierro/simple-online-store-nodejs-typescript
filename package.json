{
  "name": "programacion-backend-coder",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/index.ts --port=8080",
    "start": "node dist/index.js --port=8080",
    "dev-cluster": "nodemon src/index.ts --port=8080 --mode=cluster",
    "start-cluster": "node dist/index.js --port=8081 --mode=cluster",
    "pm2": "pm2 start dist/index.js --watch -i max",
    "stop-pm2": "pm2 delete all",
    "forever": "forever start dist/index.js --watch",
    "stop-forever": "forever stopall",
    "artillery-without-console-log": "artillery quick --count 50 -n 20 http://localhost:8080/api/info > profiling/artillery-without-console-log.txt",
    "artillery-with-console-log": "artillery quick --count 50 -n 20 http://localhost:8080/api/info?log=true > profiling/artillery-with-console-log.txt",
    "autocannon-without-console-log": "autocannon -c 100 -d 20 http://localhost:8080/api/info",
    "autocannon-with-console-log": "autocannon -c 100 -d 20 http://localhost:8080/api/info?log=true",
    "start:prof": "node --prof dist/index.js",
    "start:inspect": "node --inspect dist/index.js",
    "start0x": "0x dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahierro/programacion-backend-coder.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahierro/programacion-backend-coder/issues"
  },
  "homepage": "https://github.com/ahierro/programacion-backend-coder#readme",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "lodash": "^4.17.21",
    "log4js": "^6.7.1",
    "mongoose": "^6.7.3",
    "normalizr": "^3.6.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "uuid": "^9.0.0",
    "yargs": "^17.6.2"
  }
}
